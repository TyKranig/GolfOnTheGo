<?php
    error_reporting(-1);
ini_set('display_errors','On');

include('dbConnection.php');

$guildName = $_GET['guildName'];

"Select userName, password From db309amc1.user dude
              inner join db309amc1.guildMember member
                 on dude.userId = member.userID
              Where guildName = $guildName;getGuildMembers($guildName);

function getGuildMembers($guildName){
    global $con;
    $query = "Select userName, password From db309amc1.user dude
              inner join db309amc1.guildMember member
                 on dude.userId = member.userID
              Where guildName = $guildName;";
    
    $result = mysqli_query($con, $query);

    $rows = array();
    while($r = mysqli_fetch_assoc($result)) {
        $rows[] = $r;
    }

    echo json_encode($rows);

    $con->close();
}
?>

